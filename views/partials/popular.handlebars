<div class="panel panel-default ">
    <div class="panel-heading">
        <h3 class="panel-title">Most popular</h3>
    </div>
    <div class="panel-body" style="display:flex; flex-wrap: wrap;" id="popular-img-box">

        {{#each popularImages}}
            <div class="col-md-6  col-xs-6   group-pics full-section">
                <a href="/image/{{linkId}}"><img src="/uploads/thumbnails/{{filename}}" alt="#" class="img-responsive"></a>
            </div>
        {{/each}}

    </div>
</div>
