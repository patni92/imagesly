<div class="panel panel-default ">
    <div class="panel-heading">
        <h2 class="panel-title">Most popular</h2>
    </div>
    <div class="panel-body " id="popular-img-box">

        {{#each popularImages}}
            <div class="col-md-6  col-xs-6 text-center popular-images full-section">
                <a href="/image/{{linkId}}"><img src="/uploads/thumbnails/{{filename}}" alt="#" style="min-height: 110px;" class="img-thumbnail "></a>
            </div>
        {{/each}}

    </div>
</div>
